<script type="text/javascript">
    //判断库存
    var getStockDataName =  function(type ,data) {

      //根据type类型获取本地存储的数据名字
      //没有范围普通库存
      var json = {
        0: 'stockData',
        1: 'stockData'
      };
      if (data.bigTapJson[type]) {
        json = data.bigTapJson[type];
      }
      return json;

    };
    var getStockData = function(type, num , data) {
      //根据type类型 获取本地存储的库存数据
      return getStockDataName(type , data)[num];
    };

    var checkStock = function (json , data) {
        // 如果没有子孩子则直接查询是否有库存
        // 否则遍历子孩子
        if(!json.list){
           
            var goods = json.goods;
            
            if (goods === undefined) {
                return false;
            }
            
            var id = goods ? goods.goods_id : json.id;
            var act = goods.action;
            var stock = act.isBigtap ? getStockData(act.type, 0 , data) :
            getStockData(act.type, 1 , data);
            stock = act.isBigtap ? data[stock].mainpro : data[stock];
            if(act.type === "bigtap" || act.type === "presales" || act.type === "booking"){

                if(stock && stock[id]){
                    if( act.isBigtap ){
                        if( stock[id].hdstatus === 2){
                            return true;
                        }else{
                            return false;
                        }
                    }else{
                        if(stock[id]){
                            return true;
                        }else{
                            return false;
                        }
                    }
                }else{
                    return false;
                }
            } else{
                if(act.type !== "common"){
                    return true;
                }else{
                    return stock && stock[id];                   
                }

            }
              
        }else{
            var _stock = false;
            var list = json.list;
            for (var i = 0 , l = list.length; i < l; i++) {
                if(list[i].hasChild === undefined){
                     _stock = checkStock(list[i] , data);
                    if(_stock){
                        break ;
                    }                   
                }else if(list[i].hasChild !== undefined && list[i].hasChild){
                     _stock = checkStock(list[i] , data);
                    if(_stock){
                        break ;
                    }                   
                }

            }
    
            if(_stock){
                return true;
            }
            
        }
        return false; 
    }

    //获取套装每个产品套装库存
    var getSuitBigtapStock = function (idArr,bigtapStock,hdstatus) {
        var sign = true;
        for (var i = 0; i < idArr.length; i++) {
            if(!(bigtapStock && bigtapStock[idArr[i]] && bigtapStock[idArr[i]].hdstatus == hdstatus) ){
                sign = false;
            }
        }
        return sign;
    }
    //获取套装库存
    var getSuitStock = function (idArr,bigtapStock,hdstatus) {
    
        for (var i = 0; i < idArr.length; i++) {
            if(bigtapStock && bigtapStock[idArr[i]] && bigtapStock[idArr[i]].hdstatus == hdstatus){
                return true;
            }
        }
        return false;
    }

    //检查套装库存
    var checkSuitStock = function (json,config) {
        var goods = json.goods_info[0];
        if (!goods) {
            return false;
        }

        var id = goods.commodity_id;
        var act = goods.action;
        var mode = act.sale_mode;
        var stock = act.isBigtap ? getStockData(mode, 0 , config ) :
        getStockData(act.type, 1 , config);
        stock = act.isBigtap ? config[stock].suitpro : config[stock];
        if(mode === "bigtap" || mode === "presales" || mode === "booking"){
            if(stock && stock[id]){ 
                if( act.isBigtap ){
                    return getSuitStock(json.goods_list,stock[id],2);
                }else{
                    if(stock[id]){
                        return true;
                    }else{
                        return false;
                    }
                }
            }else{
                return false;
            }
        } else{
            if(mode !== "common" && mode !== "standard"){
                return true;
            }else{
                return stock && stock[id];             
            }
        }
    }

    //检查子选项是否应该显示
    //当子选项与选中的版本某一属性无关联，则隐藏改子选项
    var checkSuitItemExist = function (cfg,valueId,data,bindex,oindex){
        var currentSelected = data.suitSelected[bindex];
        var selectedOption = currentSelected.prop_list;
        selectedOption = selectedOption.map(function(item,index){
            if(index == oindex)
                return {
                        prop_cfg_id : cfg,
                        prop_value_id : valueId
                    }
            return item;
        });
        if(oindex == 0){
            return true;
        }
        // console.log('selectedOption:',selectedOption);
        var exist = false;
        
        data.batch_info[bindex].goods_info.map(function(item){
            var itemExit = true;
            item.prop_list.map(function (pitem,index){
                
                if( !(pitem.prop_cfg_id == selectedOption[index].prop_cfg_id && selectedOption[index].prop_value_id == pitem.prop_value_id)){
                    itemExit = false;
                    return ;
                }
            });
            
            if(itemExit){
                exist = true; 
            }
            
        });
        return exist;
    }

    //检查版本选中状态
    var checkSuitIsSelect = function (cfg,valueId,selectData,itemIndex) {
        var sign = false; 
        if(selectData && selectData[itemIndex]){
            selectData = selectData[itemIndex];
            if(selectData.prop_list){
                selectData.prop_list.map(function (pitem,pindex){
                    if(pitem.prop_cfg_id == cfg && pitem.prop_value_id == valueId){
                        sign = true;
                        return ;
                    }
                })
            }
        }
        return sign
    }

    //检查套装版本对应商品大秒库存
    var getSuitItemBigStockStock = function (cfg,valueId,bigtapStock,suitData,hdstatus) {
        var idArr = [];
        suitData.map(function (item,index){
            item.goods_info.map(function (gitem,gindex){
                gitem.prop_list.map(function (pitem){
                    if(pitem.prop_cfg_id == cfg && pitem.prop_value_id == valueId){
                        idArr.push(gitem.goods_id);
                    }
                });
            });
        })
        return getSuitStock(idArr,bigtapStock,hdstatus);
    }

    //检查套装版本库存
    var checkSuitItemStock = function (cfg,valueId,suitData) {
   
        var commdity_good = suitData.goods_info[0];
        var id = commdity_good.commodity_id;
        var act = commdity_good.action;
        var mode = act.sale_mode;
        var stock = act.isBigtap ? getStockData(mode, 0 , suitData.config ) :
        getStockData(act.type, 1 , suitData.config);
        stock = act.isBigtap ? suitData.config[stock].suitpro : suitData.config[stock];
        if(mode === "bigtap" || mode === "presales" || mode === "booking"){

            if(stock && stock[id]){ 
                if( act.isBigtap ){
                    return getSuitItemBigStockStock(cfg,valueId,stock[id],suitData.batch_info,2);
                }else{
                    if(stock[id]){
                        return true;
                    }else{
                        return false;
                    }
                }
            }else{
                return false;
            }
        } else{
            if(mode !== "common" && mode !== "standard"){
                return true;
            }else{
                return stock && stock[id];             
            }
        }
        
        
    }

    
</script>



